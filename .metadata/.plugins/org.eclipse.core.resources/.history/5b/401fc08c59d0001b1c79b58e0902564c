package xyz.elandasunshine.cvm.init;

import net.minecraft.block.Block;
import net.minecraft.enchantment.Enchantment;
import net.minecraft.entity.Entity;
import net.minecraft.item.Item;
import net.minecraft.potion.Potion;
import net.minecraft.potion.PotionType;
import net.minecraft.util.SoundEvent;
import net.minecraft.world.biome.Biome;
import xyz.elandasunshine.capi.registry.Registry;

public interface ObjectRegister
{
	Registry<SoundEvent>  SOUND_EVENTS = new Registry<SoundEvent>();
	Registry<Block>       BLOCKS       = new Registry<Block>();
	Registry<Potion>      POTIONS      = new Registry<Potion>();
	Registry<Enchantment> ENCHANTMENTS = new Registry<Enchantment>();
	Registry<Item>        ITEMS        = new Registry<Item>();
	Registry<PotionType>  POTION_TYPES = new Registry<PotionType>();
	Registry<Entity>      ENTITIES     = new Registry<Entity>();
	Registry<Biome>       BIOMES       = new Registry<Biome>();
}
